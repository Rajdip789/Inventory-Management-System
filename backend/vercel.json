{
	"version": 2,
	"builds": [
	  {
		"src": "app.js",
		"use": "@vercel/node"
	  }
	],
	"routes": [
	  {
		"src": "/(.*)",
		"dest": "app.js",
		"methods" : [
			"GET",
			"POST",
			"PUT",
			"DELETE"
		]
	  }
	],
	"headers": [
	    {
	       "source": "/(.*)",
	       "headers": [
	          { "key": "Access-Control-Allow-Origin", "value": "https://stockmerch.vercel.app/" }
	        ]
	   },
	   {
	       "source": "https://stockmerch.vercel.app/(.*)",
	       "headers": [
	          { "key": "Access-Control-Allow-Origin", "value": "https://stockmerch.vercel.app" }
	       ]
	   }  
       ]
  }
